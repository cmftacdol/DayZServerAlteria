		<file name="boomlay.xml" type="types" />
		<file name="coupledmags.xml" type="types" />
		<file name="magazineloaders.xml" type="types" />
		<file name="rag.xml" type="types" />
		<file name="scopeandmounts.xml" type="types" />
		<file name="additionalsuppressors.xml" type="types" />
		<file name="TypesTerje1.xml" type="types" />
		<file name="TypesTerje2.xml" type="types" />
		<file name="TypesTerje3.xml" type="types" />
		<file name="TypesTerjeSkills.xml" type="types" />
		<file name="Loftd.xml" type="types" />
		<file name="boxes_types.xml" type="types" />
		<file name="windstrider.xml" type="types" />
		<file name="plague.xml" type="types" />
		<file name="crafter.xml" type="types" />
		<file name="betterbackpacks.xml" type="types" />
